-- LocalScript
task.spawn(function()
	local script = local_script_10

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	local player = game:GetService("Players")
	local lplr = player.LocalPlayer
	local human = lplr.Character:WaitForChild("HumanoidRootPart")
	local enabled = false
	local value = script.Parent.Value
	local MainSpam = "Mike HUB On Top!"
	local CustomSpam = value.Text
	local Config = "All"
	local MessageEvent = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
	
	script.Parent.MouseButton1Click:Connect(function()
			if enabled == false then
			enabled = true
			if value.Text == "" then
				repeat
					wait(0.6)
					MessageEvent:FireServer(MainSpam, Config)
				until enabled == false
			else
				repeat
					wait(0.6)
					MessageEvent:FireServer(CustomSpam, Config)
				until enabled == false
			end
			else
			enabled = false
			
			end
	end)
	
	script.Parent.MouseButton2Click:Connect(function()
		if value.Visible == false then
			value.Visible = true
			
		else
			
			value.Visible = false
		end
	end)
end)

