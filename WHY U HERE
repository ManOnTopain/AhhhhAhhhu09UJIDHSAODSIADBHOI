task.spawn(function()
	local script = local_script_7

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	local player = game:GetService("Players")
	local lplr = player.LocalPlayer
	local human = lplr.Character:WaitForChild("HumanoidRootPart")
	local enabled = false
	local config = require(script.Parent.Parent.Parent.Parent.asKSA)
	
	
	script.Parent.MouseButton1Click:Connect(function()
	
		if enabled == false then
			enabled = true
	
			for i, v in pairs(game.Players:GetPlayers()) do
	
				if v.UserId == lplr.UserId then
	
				else
					if v.Character:FindFirstChild("Highlight") then
						v.Character.Highlight:Destroy()
						
					else
	
						local ESPLOCATE = v.Character
						local ESP = Instance.new("Highlight", ESPLOCATE)
						ESP.DepthMode = Enum.HighlightDepthMod.AlwaysOnTop
						ESP.Enabled = true
						ESP.Name = "Highlight"
						ESP.FillColor = Color3.new(0,0,0)
						ESP.FillTransparency = 1
						ESP.OutlineColor = Color3.new(51,51,51)
						ESP.OutlineTransparency = 0
					end
				end
			end
		else
			enabled = false
		end
	end)
end)

