	local enabled = false
	local config = require(script.Parent.Parent.Parent.Parent.asKSA)
	local value = script.Parent.Value
	
	script.Parent.MouseButton1Click:Connect(function()
	
			if game.Players:FindFirstChild(value.Text) then
			human.CFrame = game.Players:FindFirstChild(value.Text).Character.HumanoidRootPart.CFrame
			print(game.Players:FindFirstChild(value.Text).Name)
			wait(0.1)
		else
