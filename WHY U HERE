--// Modules

local modules = {
	[configure] = function()
		local Configuration = {}
		
		local Combat = Main.Combat.Main.ScrollingFrame
		local Blatant = Main.Blatant.Main.ScrollingFrame
		local Render = Main.Render.Main.ScrollingFrame
		local Utility = Main.Utility.Main.ScrollingFrame
		local World = Main.World.Main.ScrollingFrame
		
		Configuration.RunKey = Enum.KeyCode.RightShift -- Change this to your key
		
		Configuration.Whitelisted = {"CoolBoyEli08", "YMikeOfficialYT"}
		
		Configuration.WARNING = function(Title, Text, seconds)
			local GUI = script.Parent.WarnFrame
			local TitleText = GUI.Title
			local TextText = GUI.Text
			
			GUI.Visible = true
			TitleText.Text = Title
			TextText.Text = Text
			
			task.wait(seconds)
			GUI.Visible = false
		end
		
		Configuration.CombatButton = function(Name, Call)
			local button = Instance.new("TextButton")
			button.Font = Enum.Font.SourceSans
			button.TextColor3 = Color3.new(0, 0, 0)
			button.TextScaled = true
			button.TextSize = 14
			button.TextWrapped = true
			button.BackgroundColor3 = Color3.new(0.203922, 0.203922, 0.203922)
			button.Size = UDim2.new(0.889705896, 0, 0.0390243866, 0)
			button.Visible = true
			button.Name = Name
			button.Parent = Combat
			button.Text = Name
			button.MouseButton1Click:Connect(function()
				pcall(Call)
				wait(0.1)
				pcall(nil)
			end)
		end
		
		Configuration.BlatantButton = function(Name, Call)
			local button = Instance.new("TextButton")
			button.Font = Enum.Font.SourceSans
			button.TextColor3 = Color3.new(0, 0, 0)
			button.TextScaled = true
			button.TextSize = 14
			button.TextWrapped = true
			button.BackgroundColor3 = Color3.new(0.203922, 0.203922, 0.203922)
			button.Size = UDim2.new(0.889705896, 0, 0.0390243866, 0)
			button.Visible = true
			button.Name = Name
			button.Parent = Blatant
			button.Text = Name
			button.MouseButton1Click:Connect(function()
				pcall(Call)
				wait(0.1)
				pcall(nil)
			end)
		end
		
		Configuration.RenderButton = function(Name, Call)
			local button = Instance.new("TextButton")
			button.Font = Enum.Font.SourceSans
			button.TextColor3 = Color3.new(0, 0, 0)
			button.TextScaled = true
			button.TextSize = 14
			button.TextWrapped = true
			button.BackgroundColor3 = Color3.new(0.203922, 0.203922, 0.203922)
			button.Size = UDim2.new(0.889705896, 0, 0.0390243866, 0)
			button.Visible = true
			button.Name = Name
			button.Parent = Render
			button.Text = Name
			button.MouseButton1Click:Connect(function()
				pcall(Call)
				wait(0.1)
				pcall(nil)
			end)
		end
		
		Configuration.UtilityButton = function(Name, Call)
			local button = Instance.new("TextButton")
			button.Font = Enum.Font.SourceSans
			button.TextColor3 = Color3.new(0, 0, 0)
			button.TextScaled = true
			button.TextSize = 14
			button.TextWrapped = true
			button.BackgroundColor3 = Color3.new(0.203922, 0.203922, 0.203922)
			button.Size = UDim2.new(0.889705896, 0, 0.0390243866, 0)
			button.Visible = true
			button.Name = Name
			button.Parent = Utility
			button.Text = Name
			button.MouseButton1Click:Connect(function()
				pcall(Call)
				wait(0.1)
				pcall(nil)
			end)
		end
		
		Configuration.WorldButton = function(Name, Call)
			local button = Instance.new("TextButton")
			button.Font = Enum.Font.SourceSans
			button.TextColor3 = Color3.new(0, 0, 0)
			button.TextScaled = true
			button.TextSize = 14
			button.TextWrapped = true
			button.BackgroundColor3 = Color3.new(0.203922, 0.203922, 0.203922)
			button.Size = UDim2.new(0.889705896, 0, 0.0390243866, 0)
			button.Visible = true
			button.Name = Name
			button.Parent = World
			button.Text = Name
			button.MouseButton1Click:Connect(function()
				pcall(Call)
				wait(0.1)
				pcall(nil)
			end)
		end
		
		
		
		
		return Configuration
		
	end
}

--// Scripts

-- ShiftToOpen
task.spawn(function()
	local script = shift_to_open

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end


	
	
	
	
	local UIS = game:GetService("UserInputService")
	local gui = script.Parent.Main
	local Configuration = require(script.Parent:WaitForChild("Configure"))
	local enabled = false
	
	UIS.InputBegan:Connect(function(key, proccess)
		if key.KeyCode == Configuration.RunKey then
			if enabled == false then
				enabled = true
				if not game.Lighting:FindFirstChild("Blur") then
				local blurry = Instance.new("BlurEffect")
				blurry.Parent = game.Lighting
				end
				gui.Visible = true
			else
				enabled = false
				game.Lighting.Blur:Destroy()
				gui.Visible = false
			end
		end
	end)
	
	
end)

-- Draggable
task.spawn(function()
	local script = draggable

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	script.Parent.Draggable = true
end)

