-- AltToOpen
task.spawn(function()
	local script = alt_to_open

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	local GUI = script.Parent.MikeHUB
	local UIS = game:GetService("UserInputService")
	local enabled = false
	local Configuration = require(script.Parent:WaitForChild("Keybindconfig"))
	
	
	
	UIS.InputBegan:Connect(function(key, proccess)
		if key.KeyCode == Configuration.RunKey then
			if enabled == false then
				enabled = true
				GUI.Enabled = true
			else
				enabled = false
				GUI.Enabled = false
			end
		end
	end)
	
	
end)

-- Loader
task.spawn(function()
	local script = loader

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	local lplr = game.Players.LocalPlayer
	local PRIVATE = require(script.Parent.MikeHUB.asKSA)
	
	
	local function WARN(Title, Text)
		local lplr = game.Players.LocalPlayer
		local GUI = lplr.PlayerGui:FindFirstChild("MikeHUB").WARNING
		local Titlee = GUI.Title
		local Texte = GUI.Text
		
		GUI.Visible = true
		Titlee.Text = Title
		Texte.Text = Text
		wait(6)
		GUI.Visible = false
	end
	
	if lplr.Name == PRIVATE.Privates then
		WARN("Mike Private", "Loading Enjoy! left alt to open.")
	else
		WARN("Mike", "Loading Enjoy! left alt to open.")
	end
end)

