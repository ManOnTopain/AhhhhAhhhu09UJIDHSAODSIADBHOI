			void.Anchored = true
			void.BottomSurface = Enum.SurfaceType.Smooth
			void.BrickColor = BrickColor.new(0.3137255012989044, 0.4274510145187378, 0.3294117748737335)
			void.CFrame = CFrame.fromMatrix(Vector3.new(533.9497680664062, -226.09999084472656, 598.949951171875), Vector3.new(1, 0, 0), Vector3.new(0, 1, 0), Vector3.new(0, 0, 1))
			void.Color = Color3.new(0.309804, 0.423529, 0.247059)
			void.Size = Vector3.new(2047.9000244140625, 1, 2047.9000244140625)
			void.TopSurface = Enum.SurfaceType.Smooth
			void.Name = "VoidAntiAntiVoid"
			void.Parent = workspace
				
			end
			else
			enabled = false
			if game.Workspace:FindFirstChild('VoidAntiAntiVoid') then
				game.Workspace.VoidAntiAntiVoid:Destroy()
			end
			end
	end)
	
	
end)

