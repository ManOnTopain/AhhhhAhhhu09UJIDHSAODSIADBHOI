-- LocalScript
task.spawn(function()
	local script = local_script_9

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	local player = game:GetService("Players")
	local lplr = player.LocalPlayer
	local human = lplr.Character:WaitForChild("HumanoidRootPart")
	local enabled = false
	local config = require(script.Parent.Parent.Parent.Parent.asKSA)
	
	
	script.Parent.MouseButton1Click:Connect(function()
			if enabled == false then
			enabled = true
	
			if not game.Workspace:FindFirstChild("Smooth Vibes (c)") then
				local smooth_vibesc = Instance.new("Sound")
				smooth_vibesc.SoundId = "rbxassetid://9044565954"
				smooth_vibesc.Name = "Smooth Vibes (c)"
				smooth_vibesc.Parent = workspace
				smooth_vibesc.Playing = true
				smooth_vibesc.Looped = true
			end
			if not game.Lighting:FindFirstChild("Chill") then
				local colorcorrection = Instance.new("ColorCorrectionEffect")
				colorcorrection.TintColor = Color3.new(0.960784, 0.52549, 1)
				colorcorrection.Parent = game.Lighting
				colorcorrection.Name = "Chill"
			end
			else
			enabled = false
			if game.Workspace:FindFirstChild("Smooth Vibes (c)") then
				game.Workspace["Smooth Vibes (c)"]:Destroy()
				
			end
			if game.Lighting:FindFirstChild("Chill") then
				game.Lighting.Chill:Destroy()
			end
			end
	end)
	
	
end)

