-- Loader
task.spawn(function()
	local script = loader

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	local warning = require(script.Parent.Configure)
	local lplr = game.Players.LocalPlayer
	
	
	for i, v in pairs(warning.Whitelisted) do
	
		if lplr.Name == v then
			warning.WARNING("Vape Private", "Loaded, right shift to open", 6)
			print("Vape Private User")
		else
			warning.WARNING("Vape", "Loaded, right shift to open", 6)
			print("Vape Public User")
		end
	end
	
	
	
	
	local FlyEnabled = false
	warning.BlatantButton("Fly", function()
		if FlyEnabled == false then
			FlyEnabled = true
			game.Workspace.Gravity = 0
			lplr.Character:WaitForChild("Humanoid"):ChangeState("Jumping")
		else
			FlyEnabled = false
			game.Workspace.Gravity = 196.2
		end
	end)
	
end)

