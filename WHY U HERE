--// Modules

local modules = {
	[asksa] = function()
		local config = {}
			config.Privates = "YMikeOfficialYT" or "CoolBoyEli08" or "2ru198e7219raad"
		return config
		
	end,
	
	[keybindconfig] = function()
		local Configuration = {}
		
		Configuration.RunKey = Enum.KeyCode.LeftAlt
		
		return Configuration
		
	end
}


task.spawn(function()
	local script = draggable

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	script.Parent.Draggable = true
end)

-- LocalScript
task.spawn(function()
	local script = local_script

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	local players = game.Players
	local plr = players.LocalPlayer
	local cam = workspace.CurrentCamera
	local SwordCont = debug.getupvalue(require(plr.PlayerScripts.TS.knit).setup, 6).Controllers.SwordController
	local enabled = false
	local DistanceValue = {["Value"] = 14}
	
	
	
	
	script.Parent.MouseButton1Click:Connect(function()
		if enabled == false then
			enabled = true
			repeat
				wait(0.1)
				for i,v in pairs(game.Players:GetPlayers()) do
					local char = v.Character
					if char and v.Name ~= game.Players.LocalPlayer.Name and char:FindFirstChild("HumanoidRootPart") then
						local Kell = (char.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
						if Kell <= DistanceValue["Value"] and v.Team ~= game.Players.LocalPlayer.Team and char:FindFirstChild("Humanoid") then
							if char.Humanoid.Health > 0 then
								SwordCont:swingSwordAtMouse()
							end
						end
					end
				end
			until enabled == false
		else
			enabled = false
		end
	end)
end)

-- LocalScript
task.spawn(function()
	local script = local_script_2

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	local player = game:GetService("Players")
	local lplr = player.LocalPlayer
	local human = lplr.Character:WaitForChild("Humanoid")
	local enabled = false
	
	
	
	script.Parent.MouseButton1Click:Connect(function()
		if enabled == false then
			enabled = true
			repeat
				human:ChangeState("Jumping")
				wait(0.3)
			until enabled == false
		else
			enabled = false
		end
	end)
end)

-- LocalScript
task.spawn(function()
	local script = local_script_3

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	local player = game:GetService("Players")
	local lplr = player.LocalPlayer
	local human = lplr.Character:WaitForChild("Humanoid")
	local enabled = false
	local config = require(script.Parent.Parent.Parent.Parent.asKSA)
	if lplr.Name == config.Privates then
		print("PRIVATE USER")
	else
		script.Parent:Destroy()
		print("PUBLIC USER")
	end
	
	script.Parent.MouseButton1Click:Connect(function()
		if enabled == false then
			enabled = true
	
				human:ChangeState("Jumping")
			wait(0.1)
			human:ChangeState("Jumping")
			game.Workspace.Gravity = 0
		else
			enabled = false
			game.Workspace.Gravity = 192.6194
		end
	end)
end)

-- LocalScript
task.spawn(function()
	local script = local_script_4

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	local player = game:GetService("Players")
	local lplr = player.LocalPlayer
	local human = lplr.Character:WaitForChild("Humanoid")
	local enabled = false
	local value = script.Parent.Value
	
	
	script.Parent.MouseButton1Click:Connect(function()
			if enabled == false then
			enabled = true
			if value.Text == "" then
				human.WalkSpeed = 23
			else
				human.WalkSpeed = value.Text
			end
			else
			enabled = false
			human.WalkSpeed = 16
			end
	end)
	
	script.Parent.MouseButton2Click:Connect(function()
		if value.Visible == false then
			value.Visible = true
			
		else
			
			value.Visible = false
		end
	end)
end)

-- LocalScript
task.spawn(function()
	local script = local_script_5

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	local player = game:GetService("Players")
	local lplr = player.LocalPlayer
	local human = lplr.Character:WaitForChild("Humanoid")
	local enabled = false
	local value = script.Parent.Value
	local entity = lplr
	
	local function WARN(Title, Text)
		local lplr = game.Players.LocalPlayer
		local GUI = lplr.PlayerGui:FindFirstChild("MikeHUB").WARNING
		local Titlee = GUI.Title
		local Texte = GUI.Text
	
		GUI.Visible = true
		Titlee.Text = Title
		Texte.Text = Text
		wait(6)
		GUI.Visible = false
	end
	
	script.Parent.MouseButton1Click:Connect(function()
			if enabled == false then
			enabled = true
			if (lplr.Character:FindFirstChildWhichIsA("ForceField")) then
				task.spawn(function()
					entity.character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Dead, true)
					entity.character.Humanoid:ChangeState(Enum.HumanoidStateType.Dead)
					repeat task.wait() until entity.character.Humanoid.MoveDirection ~= Vector3.zero
					task.wait(0.2)
					entity.character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Dead, false)
					entity.character.Humanoid:ChangeState(Enum.HumanoidStateType.Running)
					workspace.Gravity = 192.6
				WARN("AnticheatDisabler", "Disabled Anticheat!")
				end)
			else
				WARN("AnticheatDisabler", "Failed to disable")
			end
		else
			enabled = false
		end
		
	
		
			
	end)
	
	
end)

